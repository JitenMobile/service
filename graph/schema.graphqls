# GraphQL schema example

type Query {
  word(word: String!): Word
}

type Word {
  id: ID!
  word: String!
  synonyms: [String!]
  antonyms: [String!]
  relatedTerms: [String!]
  pronunciation: String!
  tenses: Tenses
  definitions: [Definition!]!
}

type Definition {
  examples: [String!]!
  meaning: String!
  partOfSpeech: String!
}

type Tenses {
  continuous: String
  future: String
  past: String
  present: String
}
