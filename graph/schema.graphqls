# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Query {
  word(word: String!): Word
}

type Word {
  id: ID!
  word: String!
  synonyms: [String!]
  antonyms: [String!]
  relatedTerms: [String!]
  pronunciation: String
  tense: String
  definitions: [Definition!]
}

type Definition {
  id: ID!
  examples: [String!]
  meaning: String
  partOfSpeech: String
}
